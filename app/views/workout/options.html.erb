<div class="options-banner-section">
  <div class="container vertical-center" , id="header-container">
    <h1 id="header-title">Great, <%= current_user.first_name if current_user %>
      now let's take a look at the muscle groups you can workout today.</h1>
    <p id="header-subtitle">What do you feel like doing today? </p>
  </div>
</div>

<div class="jumbotron" id="form">
  <% @resulting_workouts.each do |workout| %>
  <ul><%= link_to @workout_names[workout],results_path(workout) %></ul>
<% end %>
</div>
